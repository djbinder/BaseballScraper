@{ ViewData["Title"] = "Home Page"; }

<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>Home Page</title>
</head>

<body>

    <a href="/player/form">player / form</a>
    <a href="/player/form/notes">player / form / notes</a>
    <a href="/notes">notes</a>
    <br>
    <h4>Twitter Tests</h4>
    <table>
        <tr>
            <th>Button</th>
            <th>Purpose</th>
        </tr>
        <tr>
            <td>
                <a href="/consumerkey">
                    <button>Click</button>
                </a>
            </td>
            <td>Twitter Consumer Key</td>
        </tr>
        <tr>
            <td>
                <a href="/consumersecret">
                    <button>Click</button>
                </a>
            </td>
            <td>Twitter Consumer Secret</td>
        </tr>
        <tr>
            <td>
                <a href="/accesstoken">
                    <button>Click</button>
                </a>
            </td>
            <td>Twitter Access Token</td>
        </tr>
        <tr>
            <td>
                <a href="/accesstokensecret">
                    <button>Click</button>
                </a>
            </td>
            <td>Twitter Access Token Secret</td>
        </tr>
        <tr>
            <td>
                <a href="/playersearch">
                    <button>Click</button>
                </a>
            </td>
            <td>Twitter Player Search</td>
        </tr>
    </table>

    <br>
    <hr>
    <h4>Airtable Tests</h4>
    <table>
        <tr>
            <th>Button</th>
            <th>Purpose</th>
        </tr>
        <tr>
            <td>
                <a href="/airtablekey">
                    <button>Click</button>
                </a>
            </td>
            <td>Airtable Api Key</td>
        </tr>
        <tr>
            <td>
                <a href="/managers">
                    <button>Click</button>
                </a>
            </td>
            <td>Airtable Managers</td>
        </tr>
    </table>

    <br>
    <hr>


    <h2>Airtable Config</h2>
    <p>Api Key: @ViewData["ApiKey2"]</p>
    <hr>

    <h2>Twitter Config</h2>
    <p>C Key: @ViewData["ConsumerKey"]</p>
    <p>C Secret: @ViewData["ConsumerSecret"]</p>
    <p>A Token: @ViewData["AccessToken"]</p>
    <p>A Secret: @ViewData["AccessTokenSecret"]</p>

    <hr>





    <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
        crossorigin="anonymous"></script>
    <script src="site.js"></script>
</body>

</html>